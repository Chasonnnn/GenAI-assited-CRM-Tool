{
  "name": "my-v0-project",
  "version": "0.3.0",
  "packageManager": "pnpm@10.28.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "gen:stages": "cd ../.. && python3 scripts/gen_stage_map.py",
    "gen:surrogate-contracts": "if [ -x ../api/.venv/bin/python ]; then cd ../api && .venv/bin/python ../../scripts/gen_surrogate_contracts.py; else echo 'Skipping surrogate contract generation (apps/api/.venv missing)'; fi",
    "prebuild": "pnpm run gen:stages && pnpm run gen:surrogate-contracts",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "NODE_OPTIONS=--localstorage-file=/tmp/vitest-localstorage vitest run",
    "test:watch": "NODE_OPTIONS=--localstorage-file=/tmp/vitest-localstorage vitest",
    "test:integration": "NODE_OPTIONS=--localstorage-file=/tmp/vitest-localstorage vitest run --config vitest.integration.config.ts",
    "test:all": "pnpm run test && pnpm run test:integration",
    "check": "pnpm run typecheck && pnpm run lint && pnpm run test",
    "analyze": "next experimental-analyze"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@fullcalendar/core": "^6.1.20",
    "@fullcalendar/daygrid": "^6.1.20",
    "@fullcalendar/interaction": "^6.1.20",
    "@fullcalendar/react": "^6.1.20",
    "@fullcalendar/timegrid": "^6.1.20",
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.90.20",
    "@tiptap/extension-image": "^3.18.0",
    "@tiptap/extension-link": "^3.18.0",
    "@tiptap/extension-placeholder": "^3.18.0",
    "@tiptap/extension-text-align": "^3.18.0",
    "@tiptap/extension-underline": "^3.18.0",
    "@tiptap/react": "^3.18.0",
    "@tiptap/starter-kit": "^3.18.0",
    "@types/d3-scale": "^4.0.9",
    "@types/react-simple-maps": "^3.0.6",
    "autoprefixer": "^10.4.24",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.1.1",
    "d3-scale": "^4.0.2",
    "date-fns": "4.1.0",
    "dompurify": "^3.3.1",
    "embla-carousel-react": "8.6.0",
    "emoji-picker-react": "^4.18.0",
    "html-to-image": "^1.11.13",
    "input-otp": "1.4.2",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "next-themes": "^0.4.6",
    "qrcode.react": "^3.1.0",
    "react": "19.2.4",
    "react-day-picker": "9.13.0",
    "react-dom": "19.2.4",
    "react-dropzone": "^14.4.0",
    "react-hook-form": "^7.71.1",
    "react-resizable-panels": "^4.5.6",
    "react-simple-maps": "^3.0.0",
    "recharts": "^3.7.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@next/bundle-analyzer": "16.1.6",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.1.0",
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "jsdom": "^27.4.0",
    "msw": "^2.12.7",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.10",
      "@types/react-dom": "19.2.3",
      "d3-color": ">=3.1.0",
      "minimatch": "10.2.1",
      "rollup": ">=4.59.0"
    },
    "packageExtensions": {
      "react-simple-maps@*": {
        "peerDependencies": {
          "react": "^16.8.0 || 17.x || 18.x || 19.x",
          "react-dom": "^16.8.0 || 17.x || 18.x || 19.x"
        }
      }
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "19.x",
        "react-dom": "19.x"
      }
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "lightningcss",
      "msw",
      "sharp"
    ]
  }
}
